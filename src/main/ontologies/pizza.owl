    <!-- http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_CaperTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_CaperTopping">

    <!-- http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_AmericanHotPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_AmericanHotPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_JalapenoPepperTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_MozarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_PepperoniTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#hasTopping"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_JalapenoPepperTopping"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_MozarellaTopping"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_PepperoniTopping"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_TomatoTopping"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_AmericanaPizza -->


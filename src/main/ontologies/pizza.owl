<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#"
     xml:base="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:pizza="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza">
        <rdfs:comment>A pizza ontology that describes various pizzas based on their toppings.</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#hasBase -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#hasBase">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#hasIngreedient"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#isBaseOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_Pizza"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_PizzaBase"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#hasCountryOfOrigin -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#hasCountryOfOrigin">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#hasIngreedient -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#hasIngreedient">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#isIngredientOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#hasSpiciness -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#hasSpiciness">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_SpicinessValuePartition"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#hasTopping -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#hasTopping">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#hasIngreedient"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#isToppingOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_Pizza"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_PizzaTopping"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#isBaseOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#isBaseOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#isIngredientOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_PizzaBase"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_Pizza"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#isIngredientOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#isIngredientOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#isToppingOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#isToppingOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#isIngredientOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_PizzaTopping"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_Pizza"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#hasCalorificContentValue -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#hasCalorificContentValue">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:DatatypeProperty>
    


    <!-- 

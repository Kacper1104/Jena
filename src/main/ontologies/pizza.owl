    <!-- http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_CheeseTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_CheeseTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_PizzaTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_CheesyPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_CheesyPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#hasTopping"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_CheeseTopping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_Country -->

    <owl:Class rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_Country">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#America"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#England"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#France"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#Germany"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#Italy"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_DeepPanBase -->

    <owl:Class rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_DeepPanBase">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_PizzaBase"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_ThinAndCrispyBase"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_FourCheesePizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_FourCheesePizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_Pizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#hasTopping"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">4</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_CheeseTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_GreenPepperTopping -->


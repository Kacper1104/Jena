    <!-- http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_HighCaloriePizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_HighCaloriePizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#hasCalorificContentValue"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">400</xsd:minInclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_LowCaloriePizza"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_Hot -->

    <owl:Class rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_Hot">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_SpicinessValuePartition"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_InterestingPizza -->


    <!-- http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_AnchovyTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_AnchovyTopping">

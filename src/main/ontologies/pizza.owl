                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_MozarellaTopping"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/kacper/ontologies/2022/2/pizza#kk_PepperoniTopping"/>
